{
    "name": "capeandbay/allcommerce-web",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "backpack/crud": "4.1.*",
        "backpack/revise-operation": "^1.0",
        "consoletvs/charts": "6.*",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "goldspecdigital/laravel-eloquent-uuid": "^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "ixudra/curl": "^6.17",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.5",
        "laravel/vapor-cli": "^1.10",
        "laravel/vapor-core": "^2.9",
        "laravel/vapor-ui": "^1.0",
        "lorisleiva/laravel-actions": "^1.1",
        "silber/bouncer": "v1.0.0-rc.9",
        "spatie/laravel-activitylog": "^3.16",
        "spatie/laravel-event-sourcing": "^4.6",
        "staudenmeir/eloquent-json-relations": "^1.1"
    },
    "require-dev": {
        "backpack/generators": "^3.1",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laracasts/generators": "^2.0",
        "laravel/telescope": "^4.3",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
